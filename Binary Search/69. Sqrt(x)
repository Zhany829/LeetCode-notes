跳出循环时候r总是比l小1，而我们要向下取值所以返回r
class Solution {
    public int mySqrt(int x) {
        int l = 1, r = x;
        while (l <= r) {
            int mid = l + (r-l)/2;
            int square = x/mid;
            if (mid < square) l = mid+1;
            else if (mid > square) r = mid-1;
            else return mid;
        }
        return r;
    }
}
